{
  "name": "tsexpress",
  "version": "1.0.0",
  "description": "",
  "main": "bundle.js",
  "type": "commonjs",
  "scripts": {
    "test1": "echo \"Error: no test specified\" && exit 1",
    "start-tsnode": "ts-node -r tsconfig-paths/register src/app.ts",
    "start": "rollup -c -w --bundleConfigAsCjs",
    "build": "rollup -c --bundleConfigAsCjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "build:prod": "npm run createProdDir && rollup -c && npm run minify:prod",
    "minify": "terser dist/bundle.js --output dist/bundle.min.js --compress --mangle",
    "minify:prod": "terser dist/bundle.js --output production/bundle.min.js --compress --mangle",
    "createDistDir": "npx mkdirp dist",
    "createProdDir": "npx mkdirp production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.2",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "rollup": "^4.5.0",
    "terser": "^5.24.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
